<input type="text" value="http://rkubapl.malopolska.pl/cdn/<%= zdjecie %>" id="myInput">
<button onclick="myFunction()">Kopiuj</button>
<script>
function myFunction() {
  var copyText = document.getElementById("myInput");
  copyText.select();
  document.execCommand("copy");
  alert("Copied the text: " + copyText.value);
}
</script>