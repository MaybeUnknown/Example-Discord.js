<!-- Page Content -->
<div class="container">
    <div class="card border-0 shadow my-5">
      <div class="card-body p-5">
        <h1 class="font-weight-light">Dashboard</h1>
        <p class="lead"></p>
        <p class="lead">Scroll down...</p>
        <% user.guilds.forEach(guild => {
           const permsOnGuild = new perms(guild.permissions);
    if(!permsOnGuild.has("MANAGE_GUILD")) return;
           %>
        <div class="card">
            <div class="card-body">
              <h5 class="card-title"><%= guild.name %></h5>
              <% if(client.guilds.get(guild.id)) {%> 
            <a href="/dashboard/<%= guild.id %>" class="btn btn-primary">Zarządzaj serwerem</a>
            <% } else { %>
<a href="https://discordapp.com/oauth2/authorize?client_id=<%= client.user.id %>&scope=bot&guild_id=<%= guild.id %>&response_type=code&redirect_uri="http://rkubapl.malopolska.pl/info" class="btn btn-danger">Dodaj bota na swój serwer</a>
            <% } %>
            </div>
          </div>
         <% }) %>
   <br>
    <br>
    <center>
    <p class="lead">Created by <a href="https://startbootstrap.com/">StartBootstrap</a></p>
    <p id="cookieMessage"></p>
    </center>
    <script>
        let cookieMessage = document.getElementById("cookieMessage");
        if(localStorage.getItem("cookieAccept") !== "true") {
          let cookieHTML = `
    <p id="ciasto">Ta strona uzywa <a href="http://wszystkoociasteczkach.pl/">Ciasteczek (kliknij po info)</a> <button type="button" class="btn btn-outline-success" onclick="cookieAccept();">Akceptuje</button>  <a href="http://google.com" class="btn btn-outline-danger">Nie akceptuje</a></p>
          `;
          cookieMessage.innerHTML = cookieHTML;
        }
        function cookieAccept() {
          localStorage.setItem("cookieAccept", "true");
          cookieMessage.innerHTML = "";
        }
      </script>


      </div>
    </div>
  </div>